<rt-require dependency='./Button' as='Button' />

<form id='login-form' class='ui form'>
  <div class='field'>
    <label>Email:</label>
    <input name='email' type='text'>
  </div>
  <div class='field'>
    <label>Password:</label>
    <input name='password' type='password'>
  </div>
  <Button
    onClick='{ this.props.onSubmit }'
  >
    Login
  </Button>
</form>
